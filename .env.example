# Example .env file for Dialogue-XAI-APP
# Copy this file to .env and customize it for your needs

# API Keys
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_ORGANIZATION_ID=your_openai_org_id_here
OPENAI_MODEL_NAME=gpt-4o-mini
OPENAI_MINI_MODEL_NAME=gpt-4o-mini
OPENAI_REASONING_MODEL_NAME=o1-mini
OPENAI_EMBEDDING_SIZE=1536

# Application configuration
# Set the dataset/model configuration to use
# This overrides the setting in global_config.gin
XAI_CONFIG_PATH=./configs/diabetes-config.gin

# XAI Agent Configuration
# Options: False, "o1", "mape_k", "mape_k_4", "mape_k_2", "unified_mape_k", "mape_k_unified", 
#          "mape_k_approval_2", "mape_k_approval", "mape_k_approval_4", "mape_k_openai", 
#          "mape_k_openai_2", "mape_k_openai_unified", "simple_openai"
XAI_USE_LLM_AGENT=mape_k_openai_unified

# Database Configuration
PG_VEC_CONNECTION_STRING=postgresql://username:password@localhost:5432/dbname
DB_NAME=llm-agents-db

# Threading Configuration
BACKGROUND_EXECUTOR_THREADS=10
ML_EXECUTOR_THREADS=10