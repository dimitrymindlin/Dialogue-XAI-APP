    "Purpose": "CreditPurpose",
    "Saving accounts": "SavingAccounts",
    "Checking account": "CheckingAccount",
    "Credit amount": "CreditAmount"
  },
  "drop_columns": [
    "CheckingAccount"
  ],
  "random_search_params": {
    "n_iter": 10,
    "cv": 5,
    "scoring": "accuracy",
    "n_jobs": -1,
    "verbose": 1
  },
  "dataset_path": "./german_raw_short.csv"
