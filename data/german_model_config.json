{
  "target_col": "Risk",
  "columns_to_encode": [
    "AgeGroup",
    "Gender",
    "HousingType",
    "CreditPurpose"
  ],
  "ordinal_mapping": {
    "Risk": {
      "good": 1,
      "bad": 0
    },
    "CreditDuration": {
      "Short-term (3-12 months)": 0,
      "Medium-term (13-36 months)": 1,
      "Long-term (37-72 months)": 2
    },
    "JobLevel": {
      "unskilled and non-resident": 0,
      "unskilled and resident": 1,
      "skilled": 2,
      "highly skilled": 3
    },
    "SavingAccounts": {
      "little": 0,
      "moderate": 1,
      "quite rich": 2,
      "rich": 3
    }
  },
  "model_params": {
    "n_estimators": [
      100,
      200,
      500
    ],
    "max_depth": [
      5,
      10
    ],
    "min_samples_split": [
      2,
      3
    ],
    "min_samples_leaf": [
      1,
      2
    ]
  },
  "rename_columns": {
    "Age": "AgeGroup",
    "Sex": "Gender",
    "Job": "JobLevel",
    "Housing": "HousingType",
